var assert = {
	equals:function(message, results) {
        var square = new RedOrGreen("square");
        	
        if (message === results) {
        	square.makeGreen();
	    	document.getElementById("square").style.backgroundColor = "green";
	    }
	    else {
	    	square.makeRed();
	    	document.getElementById("square").style.backgroundColor = "red";
	    }
	}
}

     var TestMyCode = {
	    run: function(name,test) {
	    	this.name = name;
	   		test(assert);
     }
 };

